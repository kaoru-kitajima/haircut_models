<launch>
  <rosparam command="load" file="$(find fusion2urdf)/launch/Assembly_copy_controller.yaml"/>
  <node args="rev2_position_controller rev1_position_controller rev6_position_controller rev4_position_controller rev5_position_controller rev3_position_controller joint_state_controller" name="controller_spawner" ns="Assembly_copy" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
  <node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
    <remap from="/joint_states" to="/Assembly_copy/joint_states"/>
  </node>
</launch>
